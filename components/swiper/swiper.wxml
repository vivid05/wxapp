<!--components/swiper/swiper.wxml-->
<swiper
bindtap="getcurrent" bindchange="bindchange" indicator-dots="true" circular="true" data-curent="current"
autoplay="true" interval="3000" duration="500" indicator-active-color="#9c26b0" easing-function="easeInOutCubic"
>
<swiper-item wx:for="{{arrar}}" wx:key='{{index}}'>
  <image mode="aspectFill" src="{{item}}" class="slide-image"/>
</swiper-item>
</swiper>
